Algoritmo pesodolar
	Definir peso,dolar,peso_dolar Como Real;
	Escribir " ingresa una cantidad en pesos mexicanos";
	Leer peso;
	Escribir "ingresa el peso del dolar";
	Leer dolar;
	peso_dolar=peso/dolar;
	Escribir peso," pesos convertidos a dolares es:";
	
	
FinAlgoritmo

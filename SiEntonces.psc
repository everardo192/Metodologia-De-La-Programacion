Algoritmo SiEntonces
	Escribir "Dame el primer numero: ";
	Leer num1;
	Escribir "dame el segundo numero: ";
	Leer num2;
	Si num1=num2 Entonces
		Escribir "Los números son iguales";
	SiNo
		si num1>num2 Entonces
			Escribir "El mayor es ",num1;
		SiNo
			Escribir "El mayor es ",num2;
		FinSi
	FinSi
FinAlgoritmo
